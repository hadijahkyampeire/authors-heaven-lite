    "axios": "^0.21.4",
    "carbon-components": "^10.25.0",
    "carbon-components-react": "^7.25.0",
    "carbon-icons": "^7.0.7",
    "classnames": "^2.3.1",
    "cypress": "^8.5.0",
    "firebase": "^8.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebase-hooks": "^3.0.4",
    "react-redux": "^7.2.4",
    "react-redux-firebase": "^3.10.0",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3",
